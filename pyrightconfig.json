{
    "include": [
        "py/marsbar"
    ],
    
    "exclude": [
      "**/__pycache__",
    ],

    "ignore": [
      "cs"
    ],

    "pythonVersion": "2.7",
    "pythonPlatform": "Windows",

    "executionEnvironments": [
      {
        "root": "src/web",
        "pythonVersion": "3.5",
        "pythonPlatform": "Windows",
        "extraPaths": [
          "src/service_libs"
        ]
      },
      {
        "root": "src/sdk",
        "pythonVersion": "3.0",
        "extraPaths": [
          "src/backend"
        ],
        "venv": "venv_bar"
      },
      {
        "root": "src/tests",
        "extraPaths": [
          "src/tests/e2e",
          "src/sdk"
        ]
      },
      {
        "root": "src"
      }
    ]
  }





ignore cs/**